function intraday_new(){
    if(Modernizr.canvas){
        var atoolsUrl = '/sqi_atools/AnalysisTools.action?frequence=live&type=4&indicators=&compare=&mode=intraday&detached=true';
        var atoolWindow = window.open(atoolsUrl,'Analysis_Tools_2','width=1024,height=630,status=no,toolbar=no,menubar=no,scrollable=no,resizable=yes,scrollbars=no,directories=no');
        atoolWindow.focus();
    }else{
        parent.launch_atools('','live');
    }
}

function historical_new(){
    if(Modernizr.canvas){
        var atoolsUrl = '/sqi_atools/AnalysisTools.action?frequence=daily&type=1&period=year&mode=historical&detached=true';
        var atoolWindow = window.open(atoolsUrl,'Analysis_Tools_2','width=1024,height=630,status=no,toolbar=no,menubar=no,scrollable=no,resizable=yes,scrollbars=no,directories=no');
        atoolWindow.focus();
    }else{
        parent.launch_atools('');
    }
}

